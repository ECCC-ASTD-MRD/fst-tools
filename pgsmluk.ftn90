***S/P PGSMLUK 
*     
      integer function pgsmluk(fld,key,ni,nj,nk,nomvar,grtyp)
#include "impnone.cdk"
      integer key,ni,nj,nk
      real fld(ni,nj,nk)
      character*4 nomvar
      character*1 grtyp
      external fstluk
      integer fstluk


      integer ier,i
      
      ier = fstluk(fld,key,ni,nj,nk)

      if (ier.lt.0) then
        pgsmluk=ier
        return
      endif

      call prefiltre(fld,ni,nj,nomvar,grtyp)
      pgsmluk=ier
      return 
      end
      
